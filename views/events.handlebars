<div class='main-wrapper'>
	<h1 class='page-title'>{{title}}</h1>
	{{#each events}}
		<div class='event-container'>
			<div class='event-header'>
				<div class='event-title'><h1>{{ name }}</h1></div>
				<div class='event-date'><h2>{{ date }}</h2></div>
			</div>
			<div class='event-img'><img class='img-scaled' src='{{ img }}'></div><div class='event-info'>
				<div class='event-description'><p>{{ info }}</p></div>
			</div>
		</div>
	{{/each}}
</div>