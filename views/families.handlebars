<div class='main-wrapper'>
	<h1 class='page-title family-model-title'>{{title}}</h1>
	{{#each families}}
		<div class='family-container'>
			<div class='family-header'>
				<div class='family-title'><h1>{{ name }}</h1></div>
			</div>
			<div class='family-img'><img class='img-scaled' src='{{ img }}'></div>
			<div class='family-parents-wrapper'>
				<div class='family-parents'>
					<h2 class='parent-block'>Parents: </h2>
					{{#each parents}}
						<div class='parent-block'><h3>{{parent}}</h3></div>
					{{/each}}
				</div>
			</div>
			<div class='family-info'>
				<div class='family-description'><p>{{ info }}</p></div>
			</div>
		</div>
	{{/each}}
</div>